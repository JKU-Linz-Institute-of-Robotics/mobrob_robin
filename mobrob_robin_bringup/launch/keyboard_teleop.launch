<?xml version="1.0"?>
<launch>
  <!-- turtlebot_teleop_key already has its own built in velocity smoother -->
  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.2" type="double"/>
    <param name="scale_angular" value="0.2" type="double"/>
    <remap from="/cmd_vel" to="/mobrob_robin/raw_cmd_vel"/>
  </node>
  
  <include file="$(find mobrob_robin_control)/launch/velocity_smoother.launch"></include> 
</launch>
